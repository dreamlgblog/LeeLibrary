<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <zero.anytime.com.viewlibrary.QQStepView
        android:id="@+id/qq_step"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:stepMax="1000"
        app:stepCurrent="100"
        app:stepTextSize="24dp"
        android:visibility="gone"
        app:innerColor="@color/colorAccent"
        app:outerColor="@color/colorPrimaryDark"
        app:borderWidth="12dp"
        app:stepTextColor="@color/colorPrimary"/>
    <TextView
        android:id="@+id/text_view"
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/text_view2"
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/text_view3"
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>